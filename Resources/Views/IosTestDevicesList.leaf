#set("title") {
	happn â€“ List of iOS Test Devices
}

#set("additional_head_tags") {
	<style>
		table, th, td {
			border: 1px solid black;
			border-collapse: collapse;
		\}
		th, td {
			padding: 7px;
		\}
		th {
			background-color: #e0e0e0; padding: 9px;
		\}
	</style>
}

#set("body") {
	<h1>List of iOS Test Devices</h1>
	<table>
		<tr>
			<th>Name</th>
			<th>Model Name</th>
			<th>OS Version</th>
			<th>Last Seen</th>
			<th>UDID</th>
			<th>Phone Number</th>
			<th>Serial Number</th>
			<th>Wifi MAC Address</th>
			<th>Bluetooth MAC Address</th>
		</tr>
		#for(device in devices) {
			<tr>
				<td>#(device.name)</td>
				<td>#(device.modelName)</td>
				<td>#(device.osVersion)</td>
				<td>#(device.dateLastSeen)</td>
				<td>#(device.udid)</td>
				<td>#(device.phoneNumber)</td>
				<td>#(device.serialNumber)</td>
				<td>#(device.wifiMAC)</td>
				<td>#(device.bluetoothMAC)</td>
			</tr>
		}
	</table>
}

#embed("lib/Master")
